{
	"name": "inbound",
	"version": "1.0.0",
	"description": "Inbound sms api",
	"type": "module",
	"main": "app.ja",
	"engines": {
		"node": "10.4.1",
		"npm": "6.1.0"
	},
	"scripts": {
		"prestart": "npm run -s build",
		"start": "nodemon --exec \"babel-node dist/app.js\"",
		"dev": "nodemon --exec \"babel-node src/app.js\"",
		"build": "babel src --out-dir dist",
		"lint": "eslint -c .eslintrc src",
		"test": "npm run test:all",
		"dev:debug": "nodemon --inspect --exec \"npm run lint && babel-node src/app.js\"",
		"test:all": "mocha --opts ./mocha.opts __test__/e2e/**/**.spec.js",
		"test:auth": "mocha --opts ./mocha.opts __test__/e2e/rest/auth/**.spec.js",
		"test:resource": "mocha --opts ./mocha.opts __test__/e2e/rest/**/resource.spec.js"
	},
	"keywords": [
		"Advance",
		"api",
		"nodejs",
		"express"
	],
	"author": "Hamid",
	"license": "ISC",
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.6",
		"@babel/node": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.9.6",
		"@babel/register": "^7.9.0",
		"@hapi/joi": "^17.1.1",
		"apollo-errors": "^1.9.0",
		"apollo-resolvers": "^1.4.2",
		"apollo-server-express": "^2.13.0",
		"aws-sdk": "^2.986.0",
		"axios": "^0.18.1",
		"axios-cookiejar-support": "^1.0.1",
		"bluebird": "^3.7.2",
		"body-parser": "^1.19.0",
		"chai": "^4.2.0",
		"config": "^3.3.6",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^7.0.0",
		"enum": "^2.5.0",
		"env-test": "^1.0.0",
		"express": "^4.17.1",
		"express-http-proxy": "^1.6.0",
		"express-session": "^1.17.1",
		"graphql": "^14.6.0",
		"graphql-log": "^0.1.3",
		"graphql-tools": "^4.0.8",
		"html-to-text": "^8.0.0",
		"joi": "^17.2.0",
		"jsonwebtoken": "^8.5.1",
		"juice": "^8.0.0",
		"lodash": "^4.17.11",
		"mongoose": "^5.9.12",
		"morgan": "^1.10.0",
		"mysql2": "^2.3.3",
		"nodemon": "^2.0.6",
		"path": "^0.12.7",
		"pg": "^8.7.3",
		"pug": "^3.0.2",
		"q": "^1.5.1",
		"query-string": "^6.12.1",
		"redis": "^3.1.2",
		"request-promise": "^4.2.5",
		"sanitize-html": "^1.26.0",
		"sequelize": "^6.9.0",
		"slugify": "^1.4.0",
		"tough-cookie": "^4.0.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"easygraphql-tester": "^5.1.6",
